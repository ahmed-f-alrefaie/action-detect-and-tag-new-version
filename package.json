{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Dan Freeman",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^2.0.1",
    "execa": "^9.6.1",
    "glob": "^13.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.0",
    "@typescript-eslint/parser": "^8.5.0",
    "@vercel/ncc": "^0.38.4",
    "babel-jest": "^30.2.0",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "jest": "^30.0.0",
    "jest-circus": "^30.0.0",
    "jiti": "^2.6.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.9.0",
    "typescript-eslint": "^8.50.0"
  }
}
